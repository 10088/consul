<div
  class="consul-auth-method-nspace-list"
  ...attributes
>
  <table>
    <thead>
      <tr>
        <th>{{t "models.auth-method.Selector"}}</th>
        <th>{{t "models.auth-method.BindNamespace"}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each @items as |item|}}
      <tr>
        <td>{{item.Selector}}</td>
        <td>{{item.BindNamespace}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

